{
  "allowCompoundWords": true,
  "language": "en_US",
  "dictionaries": [
    "bash",
    "companies",
    "css",
    "html",
    "misc",
    "softwareTerms",
    "npm",
    "typescript"
  ],
  "ignorePaths": [
    ".vscode/",
    ".idea/",
    "**/node_modules/**",
    "**/*.lock",
    "package.json"
  ],
  "ignoreRegExpList": [
    "/\\|\\s+[T|G]{2,}\\s+\\|/g",
    "/https?:\\/\\/.*/g",
    "/github.*/g"
  ],
  "words": [
    "Kubernetes",
    "devbox",
    "direnv",
    "envrc",
    "openslo",
    "slos",
    "tada"
  ]
}